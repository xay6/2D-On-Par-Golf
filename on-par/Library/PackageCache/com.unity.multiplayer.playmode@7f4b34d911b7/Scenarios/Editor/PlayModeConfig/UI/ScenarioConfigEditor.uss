.instance-drawer{
    padding: 8px;
    padding-right:6px
}

#main-editor-instance{
    margin-left: 3px;
    margin-top: 8px;
    margin-bottom: 8px;
}
.unity-foldout__text{
    -unity-font-style: bold;
}

#main-editor-instance > #main-editor-content{
    margin-left: 8px;
}

#remote-instance-list{
    margin-bottom: 16px;
}

.warn-icon{
}

.propertyfield-icon{
    margin-left: -16px;
    max-width: 16px;
    max-height: 16px;
    min-width: 16px;
    min-height: 16px;
}

.icon{
    max-width: 16px;
    max-height: 16px;
    min-width: 16px;
    min-height: 16px;
}

.missing-packages-foldout Toggle{
    margin-left: 0px;
}


.missing-packages-foldout #unity-content{
    margin-left: 0px;
}

.missing-packages-foldout #missing-packages-button-container{
    flex-shrink: 0;
}
.missing-packages-foldout Label {
    flex-grow: 1;
    overflow: hidden;
    text-overflow: ellipsis;
}

#build-profile-button{
    margin: 0px 0px 0px 3px;
    padding-top: 0px;
    padding-bottom: 0px;
}

#EditorInstancesToggle
{
    flex-direction: row-reverse;
}

#EditorInstancesToggle > Label
{
    flex-grow: 1;
    -unity-font-style: bold;
}

#EditorInstancesToggle > .unity-toggle__input
{
    flex-grow: 0;
    margin-right: 5px;
}

Foldout > Toggle
{
    margin-left: 0px;
}

.unity-foldout__content
{
    margin-left: 15px;
}

.instances-group
{
    margin-top: 8px;
}
